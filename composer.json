{
    "name": "forknetwork/strict-phpunit",
    "description": "A simple PHPUnit extension that disallows unexpected method calls.",
    "type": "library",
    "license": "Apache-2.0",
    "authors": [
        {
            "name": "Rolf Babijn",
            "email": "mailrolf@gmail.com"
        }
    ],
    "require": {
        "phpunit/phpunit": "^7.1"
    },
    "require-dev": {
        "squizlabs/php_codesniffer": "^3.1",
        "escapestudios/symfony2-coding-standard": "^3.0",
        "slevomat/coding-standard": "^4.0"
    },
    "suggest": {
        "squizlabs/php_codesniffer": "^3.0"
    },
    "autoload": {
        "psr-4": { "ForkNetwork\\StrictPHPUnit\\": "src/" }
    },
    "autoload-dev": {
        "psr-4": {
            "ForkNetwork\\StrictPHPUnit\\Tests\\": "tests/",
            "PHP_CodeSniffer\\": "vendor/squizlabs/php_codesniffer/src/",
            "PHP_CodeSniffer\\Tests\\": "vendor/squizlabs/php_codesniffer/tests/"
        }
    }
}
